{
  "manifest_version": 2,
  "name": "__MSG_name__",
  "description": "__MSG_desc__",
  "version": "0.1.0",
  "homepage_url": "https://github.com/agudulin/zeit-badge-off",
  "default_locale": "en",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "https://zeit.co/*"
  ],
  "content_scripts": [{
    "run_at": "document_end",
    "matches": [
      "https://zeit.co/*"
    ],
    "css": [
      "index.css"
    ]
  }]
}
